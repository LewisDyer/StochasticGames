module board
    // Local state:
    // 0: choosing how many dice to roll
    // 1: rolling dice
    // 2: Moving (if allowed!)
    // 3: Deciding if there's a winner
    // 4: game over
    state : [0..4] init 0;
    
    winner : [0..2] init 0;

    [pick_dice] state=0 -> (state'=1);
    [roll] state=1 -> (state'=2);
    [move] state=2 -> (state'=3);
    [pick_wins] state=3 & !game_over -> (state'=0);

    [p1_wins] state=3 & game_over & p1_end_pos=max_space -> (state'=4) & (winner'=1);
    [p2_wins] state=3 & game_over & p2_end_pos=max_space -> (state'=4) & (winner'=2);
    
    [game_over] state=4 -> (state'=4);

endmodule


label "p1_wins" = ((state=4) & (winner=1));
label "p2_wins" = ((state=4) & (winner=2));

label "game_over" = (state=4);

label "p1_bust" = p1_bust;
label "p2_bust" = p2_bust;

rewards "no_rounds"
	[pick_dice] true: 1;
endrewards


rewards "p1_busts"
	(state=2) & (p1_bust) : 1;
endrewards


rewards "p2_busts"
	(state=2) & (p2_bust) : 1;
endrewards


